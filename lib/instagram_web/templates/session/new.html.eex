<%= form_for @changeset, Routes.session_path(@conn, :create), fn f -> %>
  <%= label f, :email %>
  <%= email_input f, :email, autofocus: true %>
  <%= error_tag f, :email %>

  <%= label f, :password %>
  <%= password_input f, :password %>
  <%= error_tag f, :password %>

  <div>
    <%= submit "Login" %>
  </div>
<% end %>
